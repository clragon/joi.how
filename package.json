{
  "name": "joihow",
  "version": "1.7.0",
  "private": true,
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.1.3",
    "axios-jsonp": "^1.0.2",
    "buttplug": "^3.1.1",
    "lodash": "^4.17.15",
    "marked": "^4.2.2",
    "raw.macro": "0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga": "^3.3.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-sparklines": "^1.7.0",
    "styled-components": "^5.3.6",
    "tone": "^14.7.77",
    "util": "^0.12.5",
    "wasgen": "^0.18.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "29.2.3",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^4.0.7",
    "@types/node": "18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-sparklines": "^1.7.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "git-tag-version": false,
    "commit-hooks": false
  },
  "scripts": {
    "start": "cross-env PORT=3006 react-scripts start",
    "build": "react-scripts build",
    "test:watch": "react-scripts test",
    "test": "jest --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "postversion": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && echo \"\n## $PACKAGE_VERSION - `date -R`\n* \" >> src/release-notes.md && echo '\n\n\n-------\nAdd release notes to src/relase-notes.md then run {npm run live}'",
    "live": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag -d v$PACKAGE_VERSION && git commit -am \"release version $PACKAGE_VERSION\" && git tag \"v$PACKAGE_VERSION\" && git push origin main --tags && git checkout develop && git merge main"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^20.0.0"
  }
}
